    "description": "Kisphp composer no update plugin",
    "type": "composer-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Marius",
            "email": "mariusbogdan83@gmail.com",
            "homepage": "http://www.kisphp.com",
            "role": "Developer, owner"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.0",
        "composer-plugin-api": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Kisphp\\": "src/"
