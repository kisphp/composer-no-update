{
    "name": "kisphp/composer-no-update",
    "keywords": ["composer", "plugin", "update", "force"],
    "description": "Kisphp composer no update plugin",
    "type": "composer-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Bogdan Rizac",
            "email": "mariusbogdan83@gmail.com",
            "homepage": "http://www.kisphp.com",
            "role": "developer"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "composer-plugin-api": "~2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.18",
        "composer/composer": "^2.0",
        "symfony/var-dumper": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "Kisphp\\": "src/"
        }
    },
    "extra": {
        "class": "Kisphp\\ComposerNoUpdater"
    }
}
