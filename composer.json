{
    "name": "kisphp/composer-no-update",
    "keywords": ["composer", "plugin", "update", "force"],
    "description": "Kisphp composer no update plugin",
    "type": "composer-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Bogdan Rizac",
            "email": "mariusbogdan83@gmail.com",
            "homepage": "http://www.kisphp.com",
            "role": "developer"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=5.5.9",
        "composer-plugin-api": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^1.9.0",
        "composer/composer": "^1.0.0",
        "symfony/var-dumper": "^3.0",
        "phpunit/phpunit": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Kisphp\\": "src/"
        }
    },
    "extra": {
        "class": "Kisphp\\ComposerNoUpdater"
    }
}
